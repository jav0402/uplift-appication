{
  "name": "uplift",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start -c",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "reset-backend": "rm backend/database.db && sqlite3 backend/database.db < backend/db_schema.sql",
    "reset-backend-win": "del backend/database.db && sqlite3 backend/database.db < backend/db_schema.sql",
    "server": "node backend/server.js",
    "full": "concurrently --raw --handle-input --kill-others \"npm run start\" \"npm run server\"",
    "full-reset": "npm run reset-backend && npm run full",
    "full-reset-win": "npm run reset-backend-win && npm run full"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@types/react": "~18.2.79",
    "axios": "^1.7.5",
    "body-parser": "^1.20.2",
    "csv-parser": "^3.0.0",
    "expo": "~51.0.22",
    "expo-av": "~14.0.6",
    "expo-constants": "~16.0.2",
    "expo-device": "~6.0.2",
    "expo-document-picker": "~12.0.2",
    "expo-font": "~12.0.9",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.21",
    "expo-sqlite": "^14.0.6",
    "expo-status-bar": "~1.12.1",
    "express": "^4.19.2",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "nativewind": "^2.0.11",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-animatable": "^1.4.0",
    "react-native-appwrite": "^0.2.2",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-gifted-charts": "^1.4.30",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-paper": "^5.12.5",
    "react-native-progress": "^5.0.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-url-polyfill": "^2.0.0",
    "sqlite3": "^5.1.7",
    "typescript": "~5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.3.0"
  },
  "private": true
}
